{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "date": {
      "type": "string",
      "format": "date"
    },
    "name": {
      "type": "string",
      "pattern": "^(?!.*[ЫЪЭЁыъэё@%&$^#])[a-zA-ZА-ЯҐЇІЄа-яґїіє0-9№\\\"!\\^\\*)\\]\\[(._-].*$"
    },
    "person_name": {
      "type": "string",
      "pattern": "^(?!.*[ЫЪЭЁыъэё@%&$^#])[А-ЯҐЇІЄа-яґїіє\\'\\- ]+$"
    },
    "sub_type": {
        "type": "object",
        "properties": {
            "title": {
                "type": "string"
            },
            "additional": {
                "type": "string"
            }
        },
        "required": [
            "title"
        ],
        "additionalProperties": false
    },
    "provided_service": {
        "type": "object",
        "properties": {
            "type": {
                "type": "string",
                "description": "Dictionary: PROVIDED_SERVICE_TYPE"
            },
            "sub_types": {
                "type": "array",
                "items": {
                    "$ref": "#/definitions/sub_type"
                },
                "minItems": 1
            }
        },
        "required": [
            "type",
            "sub_types"
        ],
        "additionalProperties": false
    },
    "address": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "description": "Dictionary: ADDRESS_TYPE"
        },
        "country": {
          "type": "string"
        },
        "area": {
          "$ref": "#/definitions/name"
        },
        "region": {
          "$ref": "#/definitions/name"
        },
        "settlement": {
          "$ref": "#/definitions/name"
        },
        "settlement_type": {
          "type": "string",
          "description": "Dictionary: SETTLEMENT_TYPE"
        },
        "settlement_id": {
            "type": "string"
        },
        "street_type": {
          "type": "string",
          "description": "street type Dictionary: STREET_TYPE"
        },
        "street": {
          "$ref": "#/definitions/name"
        },
        "building": {
          "type": "string",
          "pattern": "^[1-9]([0-9]{1,3})?(([ \\-]{1})?(?![ЫЪЭЁыъэё])[А-ЯҐЇЄа-яґїє]{1})?((\\s)?(\\\\|/)(\\s)?[1-9]([0-9]{1,3})?(([ \\-]{1})?(?![ЫЪЭЁыъэё])[А-ЯҐЇЄа-яґїє]{1})?)?$"
        },
        "apartment": {
          "type": "string"
        },
        "zip": {
          "type": "string",
          "pattern": "^[0-9]{5}$"
        }
      },
      "required": [
        "type",
        "country",
        "area",
        "region",
        "settlement",
        "settlement_type",
        "street",
        "street_type",
        "building",
        "apartment",
        "zip"
      ],
      "additionalProperties": false
    },
    "phone": {
      "type": "object",
      "properties": {
        "party_id": {
           "type": "string"
        },
        "type": {
          "type": "string",
          "description": "Dictionary: PHONE_TYPE"
        },
        "number": {
          "type": "string",
          "pattern": "^\\+38[0-9]{10}$"
        },
        "public": {
          "type": "boolean"
        }
      },
      "required": [
        "type",
        "number",
        "public"
      ],
      "additionalProperties": false
    },
    "language_skill": {
      "type": "object",
      "properties": {
        "language": {
          "type": "string",
          "description": "Dictionary: ISO_LANG_CODE"
        },
        "language_level": {
          "type": "string",
          "description": "Dictionary: LANG_LEVEL"
        }
      },
      "required": [
        "language"
      ],
      "additionalProperties": false
    },
    "retirement": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "description": "Dictionary: RETIREMENT_TYPE"
        },
        "date": {
          "$ref": "#/definitions/date"
        }
      },
      "required": [
        "type",
        "date"
      ],
      "additionalProperties": false
    },
    "gender": {
      "type": "string",
      "description": "Dictionary: GENDER"
    },
    "document": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "description": "Dictionary: DOCUMENT_TYPE"
        },
        "number": {
          "type": "string",
          "minLength": 1
        },
        "issued_by": {
          "type": "string",
          "minLength": 1
        },
        "issued_at": {
          "type": "string",
          "format": "date"
        }
      },
      "required": [
        "type",
        "number",
        "issued_by",
        "issued_at"
      ],
      "additionalProperties": false
    },
    "legalization": {
      "type": "object",
      "properties": {
        "issued_date": {
          "$ref": "#/definitions/date"
        },
        "number": {
          "type": "string",
          "minLength": 1
        },
        "institution_name": {
            "type": "string",
            "minLength": 1
        }
      },
      "required": [
        "issued_date",
        "number",
        "institution_name"
      ],
      "additionalProperties": false
    },
    "education": {
      "type": "object",
      "properties": {
        "country": {
          "type": "string",
          "description": "Dictionary: COUNTRY"
        },
        "city": {
          "$ref": "#/definitions/name"
        },
        "institution_name": {
          "$ref": "#/definitions/name"
        },
        "issued_date": {
          "$ref": "#/definitions/date"
        },
        "diploma_number": {
          "type": "string",
          "minLength": 1
        },
        "degree": {
          "type": "string",
          "description": "Dictionary: EDUCATION_DEGREE"
        },
        "speciality": {
          "$ref": "#/definitions/name"
        },
        "legalized": {
          "type": "boolean"
        },
        "legalizations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/legalization"
          }
        },
        "speciality_code": {
          "type": "string",
          "description": "Dictionary: SPECIALITY_CODE"
        },
        "form": {
          "type": "string",
          "description": "Dictionary: EDUCATION_FORM"
        }
      },
      "required": [
        "country",
        "city",
        "institution_name",
        "diploma_number",
        "degree",
        "speciality",
        "speciality_code",
        "issued_date",
        "form"
      ],
      "additionalProperties": false
    },
    "related_to": {
        "type": "object",
        "properties": {
            "type": {
                "type": "string",
                "description": "Dictionary: RELATED_TO"
            },
            "sub_type": {
                "type": "string"
            }
        },
        "required": [
            "type",
            "sub_type"
        ],
        "additionalProperties": false
    },
    "qualification": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "description": "Dictionary: QUALIFICATION_TYPE"
        },
        "related_to": {
            "$ref": "#/definitions/related_to"
        },
        "course_name": {
            "type": "string"
        },
        "form": {
            "type": "string",
            "description": "Dictionary: EDUCATION_FORM"
        },
        "duration": {
            "type": "string"
        },
        "duration_units": {
            "type": "string",
            "description": "Dictionary: DURATION_UNITS"
        },
        "results": {
            "type": "number",
            "minimum": 0,
            "exclusiveMinimum": true
        },
        "start_date": {
            "$ref": "#/definitions/date"
        },
        "end_date": {
            "$ref": "#/definitions/date"
        },
        "institution_name": {
          "$ref": "#/definitions/name"
        },
        "speciality": {
          "$ref": "#/definitions/name"
        },
        "issued_date": {
          "$ref": "#/definitions/date"
        },
        "certificate_number": {
          "type": "string",
          "minLength": 1
        },
        "valid_to": {
          "$ref": "#/definitions/date"
        },
        "additional_info": {
          "type": "string"
        }
      },
      "required": [
        "type",
        "institution_name",
        "speciality",
        "related_to",
        "course_name",
        "form"
      ],
      "additionalProperties": false
    },
    "speciality": {
      "type": "object",
      "properties": {
        "speciality": {
          "type": "string",
          "description": "Dictionary: SPECIALITY_TYPE"
        },
        "speciality_officio": {
          "type": "boolean"
        },
        "level": {
          "type": "string",
          "description": "Dictionary: SPECIALITY_LEVEL"
        },
        "qualification_type": {
          "type": "string",
          "description": "Dictionary: SPEC_QUALIFICATION_TYPE"
        },
        "attestation_name": {
          "$ref": "#/definitions/name"
        },
        "attestation_date": {
          "$ref": "#/definitions/date"
        },
        "valid_to_date": {
          "$ref": "#/definitions/date"
        },
        "certificate_number": {
          "$ref": "#/definitions/name"
        },
        "order_date": {
          "$ref": "#/definitions/date"
        },
        "order_institution_name": {
          "$ref": "#/definitions/name"
        },
        "order_number": {
          "$ref": "#/definitions/name"
        },
        "attestation_results": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [
        "speciality",
        "level",
        "order_date",
        "order_number",
        "order_institution_name",
        "attestation_results",
        "qualification_type",
        "attestation_name",
        "attestation_date",
        "certificate_number",
        "valid_to_date"

      ],
      "additionalProperties": false
    },
    "science_degree": {
      "type": "object",
      "properties": {
        "country": {
          "type": "string",
          "description": "Dictionary: COUNTRY"
        },
        "city": {
          "$ref": "#/definitions/name"
        },
        "degree": {
          "type": "string",
          "description": "Dictionary: SCIENCE_DEGREE"
        },
        "institution_name": {
          "$ref": "#/definitions/name"
        },
        "diploma_number": {
          "type": "string",
          "minLength": 1
        },
        "speciality": {
          "$ref": "#/definitions/name"
        },
        "issued_date": {
          "$ref": "#/definitions/date"
        },
        "degree_country": {
            "type": "string",
            "description": "Dictionary: COUNTRY"
        },
        "degree_city": {
            "type": "string",
            "minLength": 1
        },
        "degree_institution_name": {
            "type": "string",
            "minLength": 1
        },
        "science_domain": {
            "type": "string",
            "description": "Dictionary: SCIENCE_DOMAIN"
        },
        "speciality_group": {
            "type": "string",
            "description": "Dictionary: SPECIALITY_GROUP"
        },
        "code": {
            "type": "string",
            "minLength": 1
        },
        "degree_science_domain": {
            "type": "string",
            "description": "Dictionary: SCIENCE_DOMAIN"
        },
        "academic_status": {
            "type": "string",
            "description": "Dictionary: ACADEMIC_STATUS"
        }
      },
      "required": [
        "country",
        "city",
        "degree",
        "institution_name",
        "diploma_number",
        "speciality",
        "issued_date",
        "degree_country",
        "degree_city",
        "degree_institution_name",
        "science_domain",
        "speciality_group",
        "code",
        "degree_science_domain",
        "academic_status"
      ],
      "additionalProperties": false
    },
    "party": {
      "type": "object",
      "properties": {
        "first_name": {
          "type": "string",
          "$ref": "#/definitions/person_name"
        },
        "last_name": {
          "type": "string",
          "$ref": "#/definitions/person_name"
        },
        "second_name": {
          "type": "string",
          "$ref": "#/definitions/person_name"
        },
        "birth_date": {
          "$ref": "#/definitions/date"
        },
        "birth_country": {
          "type": "string",
          "description": "Dictionary: COUNTRY"
        },
        "birth_settlement": {
          "$ref": "#/definitions/name"
        },
        "birth_settlement_type": {
          "type": "string",
          "description": "Dictionary: SETTLEMENT_TYPE"
        },
        "citizenship": {
          "type": "string",
          "description": "Dictionary: COUNTRY"
        },
        "citizenship_at_birth": {
          "type": "string",
          "description": "Dictionary: COUNTRY"
        },
        "gender": {
          "$ref": "#/definitions/gender"
        },
        "tax_id": {
          "type": "string",
          "pattern": "^([0-9]{9,10}|[А-ЯЁЇIЄҐ]{2}\\d{6})$"
        },
        "no_tax_id": {
          "type": "boolean"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "personal_email": {
          "type": "string",
          "format": "email"
        },
        "photo": {
          "type": "string"
        },
        "language_skills": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/language_skill"
          },
          "minItems": 1
        },
        "addresses": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/address"
          },
          "minItems": 1
        },
        "retirements": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/retirement"
          },
          "minItems": 1
        },
        "documents": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/document"
          },
          "minItems": 1
        },
        "phones": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/phone"
          },
          "minItems": 1
        },
        "about_myself": {
          "type": "string"
        },
        "working_experience": {
          "type": "integer",
          "minimum": 0,
          "exclusiveMinimum": true,
          "description": "qnty of full years"
        }
      },
      "required": [
        "first_name",
        "last_name",
        "birth_date",
        "birth_country",
        "birth_settlement",
        "birth_settlement_type",
        "addresses",
        "citizenship",
        "photo",
        "gender",
        "tax_id",
        "email",
        "documents",
        "phones"
      ],
      "additionalProperties": true
    }
  },
  "type": "object",
  "properties": {
    "employee_request": {
      "type": "object",
      "properties": {
        "legal_entity_id": {
          "type": "string",
          "pattern": "^[0-9a-f]{8}(-?)[0-9a-f]{4}(-?)[0-9a-f]{4}(-?)[0-9a-f]{4}(-?)[0-9a-f]{12}$"
        },
        "division_id": {
          "type": "string",
          "pattern": "^[0-9a-f]{8}(-?)[0-9a-f]{4}(-?)[0-9a-f]{4}(-?)[0-9a-f]{4}(-?)[0-9a-f]{12}$"
        },
        "employee_id": {
          "type": "string",
          "pattern": "^[0-9a-f]{8}(-?)[0-9a-f]{4}(-?)[0-9a-f]{4}(-?)[0-9a-f]{4}(-?)[0-9a-f]{12}$"
        },
        "position": {
          "type": "string",
          "description": "Dictionary: POSITION"
        },
        "start_date": {
          "$ref": "#/definitions/date"
        },
        "end_date": {
          "$ref": "#/definitions/date"
        },
        "status": {
          "type": "string",
          "enum": [
            "NEW"
          ]
        },
        "employee_type": {
          "type": "string",
          "description": "Dictionary: EMPLOYEE_TYPE"
        },
        "employment_status": {
            "type": "object"
        },
        "employee_category": {
            "type": "string"
        },
        "position_level": {
            "type": "string"
        },
        "speciality_nomenclature": {
            "type": "string"
        },
        "dk_code": {
            "type": "string"
        },
        "provided_services": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/provided_service"
            },
            "minItems": 1
        },
        "party": {
          "$ref": "#/definitions/party"
        },
        "doctor": {
          "type": "object",
          "properties": {
            "educations": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/education"
              },
              "minItems": 1
            },
            "qualifications": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/qualification"
              },
              "minItems": 1
            },
            "specialities": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/speciality"
              },
              "minItems": 1
            },
            "science_degree": {
              "$ref": "#/definitions/science_degree"
            }
          },
          "required": [
            "educations",
            "specialities"
          ]
        }
      },
      "required": [
        "legal_entity_id",
        "position",
        "start_date",
        "status",
        "employee_type",
        "party"
      ]
    }
  },
  "required": [
    "employee_request"
  ]
}
